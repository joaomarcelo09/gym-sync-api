generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Athlete {
  id        String   @id
  name      String
  height    String
  weight    String
  biotype   String
  createdAt DateTime @map("created_at")
  updateAt  DateTime @map("updated_at")
}
